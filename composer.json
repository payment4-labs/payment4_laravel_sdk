{
    "name": "payment4/cryptogateway",
    "description": "Payment4 Crypto Payment Gateway for Laravel",
    "type": "library",
    "minimum-stability": "dev",
    "keywords": [
        "payment",
        "payment4",
        "crypto",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abbas Varanloo",
            "email": "abbasvaranloo@gamil.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Payment4\\CryptoGateway\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Payment4\\CryptoGateway\\Payment4ServiceProvider"
            ]
        }
    }
}
